{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "installCommand": "npm install && pip install yt-dlp",
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "functions": {
    "src/app/api/extract/route.ts": {
      "memory": 1024,
      "maxDuration": 60,
      "includeFiles": "bin/**"
    }
  }
} 